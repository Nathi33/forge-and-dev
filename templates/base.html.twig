    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Forge & Dev{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
    {% include 'partials/navbar.html.twig' %}

    <main class="p-0">
        {% block body %}{% endblock %}
